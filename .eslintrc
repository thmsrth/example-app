{
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": [
      "airbnb"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "arrowFunctions": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "jsx-a11y/label-has-for": 0,
      "jsx-a11y/label-has-associated-control": 0
    }
  }
  